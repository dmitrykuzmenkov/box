check process memcached
  with pidfile "/home/box/memcached/memcached.pid"
  start program = "/usr/bin/bash -l -c 'memcached -l 127.0.0.1:11211 -m 64 -c 1024 -t 4 -n 16 -f 1.05 & echo $! > /home/box/memcached/memcached.pid'"
  stop program = "/usr/bin/bash -l -c 'kill `cat /home/box/memcached/memcached.pid`'"
  if not exist then alert
